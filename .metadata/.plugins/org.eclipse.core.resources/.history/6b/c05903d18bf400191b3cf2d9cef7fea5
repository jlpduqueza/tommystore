$(function() {

    var contextPath = $(document.body).attr("data-context-path");

    $.get({

        url: contextPath + '/ajax/productlist',
        success: function(res) {

            if (res.length == 0) {
                $('#productTable').hide();
                $('.tableEmptyMessage').show();
            } else {
                $.each(res, function(key, value) {
                    $('#productTable').append(adminProductRow(value));
                });
            }

        },
        error: function(error) {
            console.log(error);
            alert("There's a problem adding shipping address.");
        }

    });

});
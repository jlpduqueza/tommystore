$(function() {

	var contextPath = $(document.body).attr("data-context-path");

	$message = $('.customMessage');
	$message.hide();
	
		$('#shippingAddressTable').on('click', '.delete-shipping-address-link', function(e) {

		    e.preventDefault(); 

			var $id = $(this).attr("id");
			var $row = "#tr"+$id;

			 $.get({
				url : contextPath+'/ajax/delete-shipping-address?id='+$id,
				success : function(res) {
					
		        	 $($row).remove();
	        		 $message.attr("class", "alert alert-success customMessage").show().html(res.customMessage);
				}
				, error:function(error){

					$message.attr("class", "alert alert-danger customMessage").show().html(res.customMessage);
					console.log(error);
				}
			
			});
   });

    
});
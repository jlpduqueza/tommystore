$(function() {

    var contextPath = $(document.body).attr("data-context-path");

    $.get({

        url: contextPath + '/ajax/productlist',
        success: function(res) {
            $.each(res, function(key, value) {
                $('.card-container').append(productCardTemplate(value));
            });
        },
        error: function(error) {
            console.log(error);
        }

    });

});